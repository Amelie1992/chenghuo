/* pages/order/order-detail/index.wxss */

page {
  min-height: 100%;
  background-color: #f4f5f9;
}

.container {
  min-height: 100%;
  overflow: hidden;
  overflow-y: hidden;
}

.sec-wrap {                                             

  background-color: #fff;
  margin-top: 20rpx;
}

.bottom-fiexd {
  position: fixed;
  bottom: 0;
  left: 0;
}

.sec-wrap .order-status {
  width: 720rpx;
  margin-left: 30rpx;
  border-bottom: 1rpx solid #eee;
  height: 140rpx;
  display: flex;
  align-items: center;
}

.order-status .icon-box {
  width: 80rpx;
  height: 80rpx;
  overflow: hidden;
  margin-right: 30rpx;
}

.order-status .icon-box .icon {
  width: 80rpx;
  height: 80rpx;
}

.order-status .right-text {
  width: 580rpx;
  overflow: hidden;
}

.order-status .right-text .status {
  font-size: 28rpx;
  color: #000;
  margin-bottom: 10rpx;
}

.order-status .right-text .red {
  color: #f40;
}

.order-status .right-text .des {
  font-size: 24rpx;
  color: #999;
}

.address-sec {
  width: 720rpx;
  margin-left: 30rpx;
  display: flex;
  align-items: center;
  padding-top: 30rpx;
}

.address-sec .icon-box {
  width: 40rpx;
  align-self: flex-start;
  overflow: hidden;
  margin-right: 30rpx;
  color: #f40;
}

.address-sec .icon-box .iconfont {
  width: 30rpx;
  height: 30rpx;
  font-size: 40rpx;
}

.icon-box icon.iconfont {
  font-size: 40rpx;
}

.address-sec  .right-box {
  width: 620rpx;
}

.address-sec  .right-box .name-tel {
  font-size: 28rpx;
  color: #000;
  margin-bottom: 20rpx;
}

.address-sec  .right-box .text {
  font-size: 24rpx;
  line-height: 36rpx;
  height: 72rpx;
  overflow: hidden;
}

.wuliu-detail {
  background: #fff;
  font-size: 28rpx;
}

.wuliu-info {
  padding: 10rpx 30rpx;
  border-bottom: 1px solid #eee;
}

.wuliu-box {
  width: 720rpx;
  margin-left: 30rpx;
  display: flex;
  align-items: center;
  padding-top: 10rpx;
  font-size: 26rpx;
  color: #666;
}

.wuliu-box .icon-box .icon {
  width: 40rpx;
  height: 40rpx;
  line-height: 40rpx;
}

.wuliu-box .arrow-right {
  width: 15rpx;
  height: 24rpx;
}

.wuliu-box .arrow-right .arrow {
  width: 15rpx;
  height: 24rpx;
}

.wuliu-box .right-text {
  width: 575rpx;
  margin-right: 30rpx;
}

.wuliu-box .right-text .order-number {
  font-size: 26rpx;
  color: #666;
  margin-bottom: 14rpx;
}

.wuliu-box .right-text .wuliu-text, .wuliu-box .right-text .wuliu-date {
  font-size: 24rpx;
  color: #888;
  line-height: 36rpx;
}

.order-info {
  margin: 20rpx auto;
  background: #fff;
  justify-content: flex-start;
  padding: 15rpx 0;
}

.order-info.goods-info .row-box {
  justify-content: flex-start;
  padding: 5rpx 30rpx;
  font-size: 26rpx;
  color: #666;
}

.goods-list {
  width: 100%;
  background-color: #fff;
  margin-top: 20rpx;
  border-bottom: 1px solid #eee;
}

.goods-list .list-title {
  font-size: 28rpx;
  color: #000;
  padding: 15rpx 0 15rpx 30rpx;
}

.goods-list  .a-goods {
  width: 720rpx;
  margin-left: 30rpx;
  display: flex;
  /*justify-content: space-between;*/
  border-top: 1px solid #eee;
  padding: 30rpx 30rpx 30rpx 0;
}

.goods-list  .a-goods .img-box {
  width: 160rpx;
  height: 160rpx;
  overflow: hidden;
  margin-right: 20rpx;
  background-color: #d8d8d8;
}

.goods-list .img-box .img {
  width: 160rpx;
  height: 160rpx;
}

.goods-list  .a-goods .text-box {
  width: 510rpx;
  box-sizing: border-box;
  padding-top: 10rpx;
}

.goods-list  .btn-row {
  width: 690rpx;
  margin-left: 30rpx;
  margin-right: 30rpx;
  font-size: 28rpx;
  border-top: 1rpx solid #eee;
}

.confirm-btn {
  background: #fff;
  border: 1rpx solid #f40;
  border-radius: 14rpx;
  box-sizing: border-box;
  width: 180rpx;
  height: 60rpx;
  line-height: 58rpx;
  text-align: center;
  margin: 20rpx 0 20rpx 20rpx;
  font-size: 26rpx;
  color: #f40;
}

.a-goods .text-box .arow {
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.a-goods .text-box .arow .goods-name {
  width: 360rpx;
  font-size: 26rpx;
  height: 74rpx;
  color: #000;
  line-height: 1.6;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}

.a-goods .text-box .arow01 {
  margin-bottom: 20rpx;
}

.a-goods .text-box .arow .goods-price {
  font-size: 26rpx;
  color: #000;
  align-self: flex-start;
}

.a-goods .text-box .arow .goods-label {
  width: 360rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-size: 26rpx;
  color: #999;
}

.a-goods .text-box .arow .goods-num {
  font-size: 26rpx;
  color: #999;
}

.peisong-way {
  width: 100%;
  background-color: #fff;
  margin-bottom: 20rpx;
}

.peisong-way .row-box {
  width: 720rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  box-sizing: border-box;
  padding: 24rpx 0;
  border-bottom: 1rpx solid #eee;
  margin-left: 30rpx;
}

.peisong-way .row-label {
  font-size: 28rpx;
  color: #000;
}

.peisong-way .right-text {
  font-size: 28rpx;
  color: #666;
  padding-right: 30rpx;
}

.peisong-way .liuyan {
  width: 510rpx;
  font-size: 28rpx;
}

.goods-info {
  width: 100%;
  background-color: #fff;
}

.goods-info .row-box {
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  box-sizing: border-box;
  padding: 10rpx 30rpx;
  font-size: 28rpx;
  color: #000;
}

.goods-info .row-box .right-text {
  text-align: right;
}

.jiesuan-box {
  display: flex;
  justify-content: space-between;
  width: 100%;
  height: 100rpx;
  position: fixed;
  bottom: 0;
  left: 0;
  border-top: 1px solid #eee;
  background-color: #fff;
  z-index: 4;
}

.jiesuan-box .to-pay-btn {
  width: 250rpx;
  text-align: center;
  height: 100%;
  line-height: 100rpx;
  background-color: #f40;
  font-size: 32rpx;
  color: #fff;
  border-radius: 0;
}

.jiesuan-box  .left-price {
  display: flex;
  width: 500rpx;
  justify-content: flex-end;
  line-height: 100rpx;
  padding: 0 30rpx 0 0;
  font-size: 28rpx;
  box-sizing: border-box;
}

.jiesuan-box .total {
  color: #f40;
  text-align: right;
}

.order-daifukuan {
  width: 100%;
  height: 100%;
  background: #f40;
  color: #fff;
  font-size: 40rpx;
  border-radius: 4px;
  display: block;
  text-align: center;
  line-height: 80rpx;
}

.order-daifahuo {
  width: 100%;
  height: 100%;
  background: #33d369;
  color: #fff;
  font-size: 40rpx;
  border-radius: 4px;
  display: block;
  text-align: center;
  line-height: 80rpx;
}

.order-daishouhuo {
  width: 100%;
  height: 100%;
  background: #2ecaf1;
  color: #fff;
  font-size: 40rpx;
  border-radius: 4px;
  display: block;
  text-align: center;
  line-height: 80rpx;
}

.order-gb {
  width: 100%;
  height: 100%;
  background: #919394;
  color: #fff;
  font-size: 40rpx;
  border-radius: 4px;
  display: block;
  text-align: center;
  line-height: 80rpx;
}

.text-ml {
  margin-left: 60rpx;
}

.sale-after {
  border: 1rpx solid #ccc;
  border-radius: 6px;
  color: #666;
}

.weui-textarea {
  height: 150rpx;
  margin-top: 10rpx;
  padding: 10rpx 20rpx;
  border: 1px solid #eee;
  width: 646rpx;
}

.sale-after-arow {
  display: flex;
  margin-left: 30rpx;
  justify-content: flex-end;
}

.col-green {
  color: #08eb53;
}
